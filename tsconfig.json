{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "outDir": "./dist",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"],
      "@e2b/*": ["e2b/src/*"]
    },
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "types": ["jest", "node"]
  },
  "include": ["supabase/**/*.ts", "tests/**/*.ts", "src/**/*.ts", "src/types/*.d.ts", "e2b/src/**/*.ts"],
  "exclude": ["node_modules"]
}