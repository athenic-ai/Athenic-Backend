{
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supabase": "^1.223.10",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@e2b/code-interpreter": "^1.1.1",
    "@e2b/sdk": "^0.12.5",
    "@inngest/agent-kit": "^0.7.3",
    "@supabase/supabase-js": "^2.39.8",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "inngest": "^3.27.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "uuid": "^9.0.1",
    "zod": "^3.24.3"
  },
  "scripts": {
    "# Testing": "",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --watch --config=jest.config.js",
    "test:coverage": "jest --coverage --config=jest.config.js",
    "test:inngest": "npx ts-node tests/inngest/test-inngest.ts",
    "test:logs": "npx ts-node tests/log-test.ts",
    "test:chat": "npx ts-node tests/inngest/test-chat-message.ts",
    "test:direct": "npx ts-node tests/inngest/test-direct-call.ts",
    "# Inngest Server": "",
    "start:inngest": "npx ts-node --project tsconfig-override.json src/inngest/server.ts",
    "dev:inngest": "npx nodemon --exec \"ts-node --project tsconfig-override.json src/inngest/server.ts\"",
    "dev:inngest-cli": "npx inngest-cli@latest dev -u http://localhost:8001/api/inngest",
    "# API Server": "",
    "start:api": "npx ts-node --project tsconfig-override.json src/api/server.ts",
    "dev:api": "npx nodemon --exec \"ts-node --project tsconfig-override.json src/api/server.ts\"",
    "# Combined Commands": "",
    "start:all": "npx concurrently \"npm run start:inngest\" \"npm run start:api\"",
    "dev:all": "npx concurrently --kill-others --prefix \"[{name}]\" --prefix-colors \"blue.bold,green.bold,yellow.bold\" --names \"INNGEST,API,DEV-CLI\" --timestamp-format \"HH:mm:ss\" \"npm run dev:inngest\" \"npm run dev:api\" \"npm run dev:inngest-cli\"",
    "# Logs": "",
    "dev:logs": "tail -f logs/*.log",
    "logs": "src/scripts/view-logs.sh",
    "logs:follow": "src/scripts/view-logs.sh --follow",
    "logs:errors": "src/scripts/view-logs.sh --grep ERROR",
    "logs:inngest": "src/scripts/view-logs.sh --log inngest --follow",
    "logs:api": "src/scripts/view-logs.sh --log api --follow"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/jest.setup.js"
    ]
  }
}
