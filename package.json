{
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supabase": "^1.223.10",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@e2b/code-interpreter": "^1.1.1",
    "@e2b/sdk": "^0.12.5",
    "@inngest/agent-kit": "^0.7.3",
    "@supabase/supabase-js": "^2.39.8",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "inngest": "^3.27.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "uuid": "^9.0.1",
    "zod": "^3.24.3"
  },
  "scripts": {
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --watch --config=jest.config.js",
    "test:coverage": "jest --coverage --config=jest.config.js",
    "test:inngest": "npx ts-node src/inngest/test-inngest.ts",
    "start:inngest": "npx ts-node src/inngest/server.ts",
    "start:inngest:js": "node src/inngest/server.js",
    "dev:inngest": "npx nodemon --exec ts-node src/inngest/server.ts",
    "dev:inngest:js": "npx nodemon src/inngest/server.js",
    "start:api": "npx ts-node src/api/server.ts",
    "start:api:js": "node src/api/server.js",
    "dev:api": "npx nodemon --exec ts-node src/api/server.ts",
    "dev:api:js": "npx nodemon src/api/server.js",
    "start:all": "npx concurrently \"npm run start:inngest\" \"npm run start:api\"",
    "start:all:js": "npx concurrently -n \"INNGEST,API\" \"node src/inngest/server.js\" \"node src/api/server.js\"",
    "dev:all": "npx concurrently \"npm run dev:inngest\" \"npm run dev:api\"",
    "dev:all:js": "npx concurrently \"npm run dev:inngest:js\" \"npm run dev:api:js\""
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/jest.setup.js"
    ]
  }
}
